{{ include('admin/rssfeed/_delete_rss_feed_confirmation.html.twig') }}
<form action="{{ url('admin_rss_feed_delete', {id: rss_feed.id}) }}" method="post" data-confirmation="true" id="delete-form">
    <input type="hidden" name="token" value="{{ csrf_token('delete') }}" />
    <button type="submit" class="btn btn-block btn-danger">
        <i class="fa fa-trash" aria-hidden="true"></i>
        {{ 'action.delete_rss_feed'|trans }}
    </button>
</form>
